
1;2cimport matplotlib.pyplot as plt


for nr in range(1,6):
    print "nr = " + str(nr)
    f = open("nr-" + str(nr) + "/test.txt", "r")
    count = 0
    a = []
    for line in f.readlines():
        if count == 50: break
        a += [line.split()[-1].split("%")[0]]
        count += 1

    print a
    print len(a)
    assert len(a) == 50
    f.close()
